  "description": "GUI browser for Skadi FotoWeb adapter",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && NODE_ENV=production webpack --progress --config ./webpack.config.prod.js",
    "client:start": "nodemon --watch webpack.config.dev.js --exec \"npm run webpack:serve\"",
    "lint": "eslint --ext .js,.jsx lib/ src/ tests/",
    "lint:watch": "esw --color --ext .js,.jsx --watch controllers/ lib/ src/",
    "test": "mocha --require ignore-styles test/setup.js test/**/*-test.js",
    "test:watch": "mocha --require ignore-styles --watch --watch-extensions jsx test/setup.js test/**/*-test.js",
    "webpack:serve": "webpack-dev-server --env.development --color --config webpack.config.dev.js"
  },
